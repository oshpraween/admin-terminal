    "i18next-browser-languagedetector": "^8.2.0",
    "node-forge": "^1.3.1",
    "react": "^19.1.0",
    "react-activation": "^0.13.4",
    "react-dom": "^19.1.0",
    "react-highlight-words": "^0.21.0",
    "react-i18next": "^15.6.0",
